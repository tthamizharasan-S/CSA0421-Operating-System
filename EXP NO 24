#include <stdio.h>
#include <string.h>

int main() {
    int n;
    char lines[100][200];
    char pattern[50];

    printf("Enter number of lines: ");
    scanf("%d", &n);
    getchar(); // clear buffer

    printf("Enter the lines:\n");
    for (int i = 0; i < n; i++) {
        fgets(lines[i], sizeof(lines[i]), stdin);
    }

    printf("\nEnter pattern to search: ");
    scanf("%s", pattern);

    printf("\nMatching Lines:\n");
    for (int i = 0; i < n; i++) {
        if (strstr(lines[i], pattern) != NULL) {
            printf("%s", lines[i]);
        }
    }

    return 0;
}
