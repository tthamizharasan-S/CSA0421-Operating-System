#include <stdio.h>
#include <fcntl.h>
#include <unistd.h>

int main() {
    int fd1, fd2;
    char buffer[100];

    fd1 = open("input.txt", O_RDONLY);
    fd2 = open("output.txt", O_WRONLY | O_CREAT, 0644);

    int n = read(fd1, buffer, sizeof(buffer));
    write(fd2, buffer, n);

    close(fd1);
    close(fd2);

    printf("File copied successfully\n");
    return 0;
}
